<html>
	<head>
		<meta charset="UTF-8" />
		<link rel="icon" href="/favicon.ico" type="image/png" />
		<title>Video Player - Wrapper: Offline</title>
		<link rel="stylesheet" type="text/css" href="/static/pages/css/modern-normalize.css">
		<link rel="stylesheet" type="text/css" href="/static/pages/css/global.css">
		<link rel="stylesheet" type="text/css" href="/static/pages/css/flash.css">
	</head>

	<body style="margin:0px">
		<main>
			<%~ it.object(it.attrs, it.params) %>
		</main>
	</body>

	<script>
		const player = document.getElementById("obj");
		function myNameIsClevelandBrown() {
			let isPlaying = true;

			document.addEventListener("keydown", (event) => {
				if (event.keyCode !== 32) return;
				if (isPlaying) {
					player.pause();
					isPlaying = false;
				} else {
					player.play();
					isPlaying = true;
				}
			});
		}

	</script>
</html>
